{{^pages}}
<p class="lead">На жаль ми нічого не знайшли</p>
{{/pages}}

{{#pages}}
<table class="table table-dark">
  <tr>
    <th scope="col">Fullname</th>
    <th scope="col">Username</th>
  </tr>
  <tbody>
    {{#list}}
    <tr>
      <td>
        <a alt="fullname" href="../users/{{username}}"><u>{{ fullname }}</u></a>
      </td>
      <td alt="username">{{ username }}</td>
    </tr>
    {{/list}}
  </tbody>
</table>
{{/pages}}